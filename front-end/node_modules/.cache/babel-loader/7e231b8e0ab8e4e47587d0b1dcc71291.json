AASA,gBAAA,KAAA,CAAA,OAAA,GAAA,EAAA,CAAA,IAAA;AATA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,KAjBA;AAkBA,IAAA,QAlBA,sBAkBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,YAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KA1BA;AA2BA,IAAA,UA3BA,sBA2BA,IA3BA,EA2BA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,KA9BA;AA+BA,IAAA,UA/BA,sBA+BA,IA/BA,EA+BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAJA,kDAKA,IALA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAxCA;AAyCA,IAAA,QAzCA,oBAyCA,IAzCA,EAyCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA;AADA,iBAAA,CAFA;;AAAA;AAKA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AANA,kDAOA,IAPA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA;AApDA;AAfA,CAAA","sourcesContent":["<template>\n<div>\n  <div class=\"admin\">\n      <h1>The Upload Page!</h1>\n    <div class=\"heading\">\n      <h2>Add a file to upload</h2>\n    </div>\n    <div class=\"add\">\n      <div class=\"form\">\n        <input v-model=\"title\" placeholder=\"File Name\">\n        <p></p>\n        <input type=\"file\" name=\"photo\" @change=\"fileChanged\">\n        \n        <button @click=\"upload\">Upload</button>\n      </div>\n      <div class=\"upload\" v-if=\"addItem\">\n        <p class=\"message\">Upload Successful! Have a better day!</p>\n      </div>\n    </div>\n  </div>\n</div>\n</template>\n\n<script>\nimport axios from 'axios';\nexport default {\n  name: 'upload',\n  data() {\n    return {\n      title: \"\",\n      file: null,\n      addItem: null,\n      items: [],\n      findTitle: \"\",\n      findItem: null,\n    }\n  },\n  created() {\n    this.getItems();\n  },\n  methods: {\n    fileChanged(event) {\n      this.file = event.target.files[0]\n    },\n    async upload() {\n      try {\n        const formData = new FormData();\n        formData.append('file', this.file, this.file.name)\n        let r1 = await axios.post('/api/documents', formData);\n        let r2 = await axios.post('/api/items', {\n          title: this.title,\n          path: r1.data.path\n        });\n        this.addItem = r2.data;\n      } catch (error) {\n        //console.log(error);\n      }\n    },\n    async getItems() {\n      try {\n        let response = await axios.get(\"/api/items\");\n        this.items = response.data;\n        return true;\n      } catch (error) {\n        //console.log(error);\n      }\n    },\n    selectItem(item) {\n      this.findTitle = \"\";\n      this.findItem = item;\n    },\n    async deleteItem(item) {\n      try {\n        await axios.delete(\"/api/items/\" + item._id);\n        this.findItem = null;\n        this.getItems();\n        return true;\n      } catch (error) {\n        //console.log(error);\n      }\n    },\n    async editItem(item) {\n      try {\n        await axios.put(\"/api/items/\" + item._id, {\n          title: this.findItem.title,\n        });\n        this.findItem = null;\n        this.getItems();\n        return true;\n      } catch (error) {\n        //console.log(error);\n      }\n    },\n  },    \n}\n</script>\n\n<style scoped>\n.message{\n  font-size: 2em;\n  color: green;\n}\n\n</style>\n"],"sourceRoot":"src/views"}]}